<tr class="time_report_project">
	<th id="time_report_project_1" class="project"><%= project.name %></th>
	<% id_prefix = 'pr' + project.id.to_s + '_' %></th>
	<% for count in 1..5 do %>
	<th id="<%= id_prefix + count.to_s %>" class="day">0</th>
	<% end %>
	<th id="pr<%= project.id %>_total" class="total_week">0</th>
</tr>