var action = "<%= @action %>";
if (action=="signed") {
	$("a.button_sign_report").addClass("active").attr('onclick','').parent().effect("highlight", {}, 3000);
	$("a.button_save_report").parent().remove();
} else if(action=="saved") {
	$("a.button_save_report").html("Saved").addClass("active").parent().effect("highlight", {}, 3000);
}